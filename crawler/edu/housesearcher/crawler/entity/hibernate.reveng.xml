<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="string"
			length="19" not-null="false">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="1"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="DOUBLE" hibernate-type="string"
			precision="22" not-null="false">
		</sql-type>
		<sql-type jdbc-type="DATE" hibernate-type="string" length="10"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="string"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string"
			not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="ent_user" match-catalog="housesearcher"></table-filter>
</hibernate-reverse-engineering>